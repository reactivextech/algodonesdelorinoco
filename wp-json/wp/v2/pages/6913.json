\n\t\t\tvar pricePerTree = window.corekees.calculateSubscriptionTreePrice(nTrees);\n\n\t\t\tconsole.log( \"Selected \" + nTrees + \" trees per month for \" + years + \" years\" );\n\t\t\t\n\t\t\tvar totalTrees = (years * 12) * nTrees;\n\t\t\tvar totalInvestment = totalTrees * pricePerTree;\n\t\t\tvar payment = treeDividendYieldRatio * totalTrees * 20; \/\/totalInvestment does not work with discounted subscriptions`\n\t\t\tvar co2Amount = treeCO2OffsetRatio * totalTrees;\n\t\t\t\n\t\t\tfunction isInt(n) { return n % 1 === 0; }\n\t\t\tif (isInt(pricePerTree * nTrees)) {\n\t\t\t\t$treePrice.text((pricePerTree * nTrees).toLocaleString('en-GB', { style: 'currency', currency: 'EUR' }).slice(0, -3));\n\t\t\t} else {\n\t\t\t\t$treePrice.text((pricePerTree * nTrees).toLocaleString('en-GB', { style: 'currency', currency: 'EUR' }));\n\t\t\t}\n\t\t\tif (isInt(totalInvestment)) {\n\t\t\t\t$investment.text(totalInvestment.toLocaleString('en-GB', { style: 'currency', currency: 'EUR' }).slice(0, -3));\n\t\t\t} else {\n\t\t\t\t$investment.text(totalInvestment.toLocaleString('en-GB', { style: 'currency', currency: 'EUR' }));\n\t\t\t}\n\t\t\t$dividends.text(\"\u20ac \" + Math.round(payment).toLocaleString('en-GB'));\n\t\t\t$amountOfYears.text(years);\n\t\t\t\t\n\t\t\tif (nTrees < 5) {\n\t\t\t\t$divperc.text((0.078).toLocaleString('en-GB', { style: 'percent', minimumFractionDigits: 1 }));\n\t\t\t} if (nTrees >= 5) {\n\t\t\t\t$treePrice.prepend('<span class=\"discount-original-price\"><s>\u20ac' + (nTrees * 20) + '<\/s><\/span>');\n\t\t\t\t$divperc.text((0.081).toLocaleString('en-GB', { style: 'percent', minimumFractionDigits: 1 }));\n\t\t\t} if (nTrees >= 10) {\n\t\t\t\t$divperc.text((0.085).toLocaleString('en-GB', { style: 'percent', minimumFractionDigits: 1 }));\n\t\t\t}\n\n\t\t\t$CO2Offset.text(Math.round(co2Amount).toLocaleString('en-GB'));\n\t\t\t$CO2FlightHours.text(Math.round(co2Amount \/ treeCO2FlightHrsRatio).toLocaleString('en-GB'));\n\n\t\t\tif (window.corekees.treeGraph !== undefined) {\n\t\t\t\twindow.corekees.treeGraph.setTreeInput(\n\t\t\t\t\tNumber(nTrees) || 1,\n\t\t\t\t\tNumber(years) || 1\n\t\t\t\t);\n\t\t\t}\n\t\t}\n\t\t\n\t\t\/* Some housekeeping *\/\n\t\tvar getUrl = url; \/\/ Base URL to which GET parameters will be added\n\t\t\n\t\t\/* Add to cart AJAX *\/\n\t\tjQuery('#treeSliderInvest').on('click touchstart', function(e){ \/\/ Invest\n\t\t\te.preventDefault();\n\t\t\tconsole.log(\"Amount submitted: \" + $treesNumber.val()); \/\/ TEST\n\t\t\tvar pricePerTree = window.corekees.calculateSubscriptionTreePrice($treesNumber.val());\n\t\t\tif ($treesNumber.val() > 0) {\n\t\t\t\tgetUrl = getUrl + \"?add-to-cart=\" + treeID + \"&quantity=\" + $treesNumber.val(); \n\t\t\t\tjQuery.ajax({\n\t\t\t\t\turl: getUrl,\n\t\t\t\t\tsuccess: function(result){\n\t\t\t\t\t\twindow.location.href = url;\n\t\t\t\t\t},\n\t\t\t\t\terror: function(req, xhr, status, err){ \n\t\t\t\t\t\tconsole.log(\"Error :\" + req + xhr + status + err); \/\/ TEST\n\t\t\t\t\t\tjQuery('#treeSliderError').slideDown();\n\t\t\t\t\t}\n\t\t\t\t});\n\t\t\t} else {\n\t\t\t\tjQuery('#treeSliderError').slideDown();\n\t\t\t}\n\t\t\tjQuery(this).addClass(\"crk-interactive\");\n\t\t\tjQuery(this).children(\".x-particle\").addClass(\"crk-interactive\");\n\t\t});\n\t\t\n\t});\n})(window);\n<\/script>\n\n\n<a class=\"e7160-14 x-anchor x-anchor-button has-particle\" tabindex=\"0\" id=\"treeSliderInvest\" >\n\n  \n    <div class=\"x-anchor-content\">\n            <div class=\"x-anchor-text\"><span class=\"x-anchor-text-primary\" >To basket<\/span><\/div>          <\/div>\n\n        \n<span class=\"x-particle is-primary\" data-x-particle=\"scale-y inside-t_c\" aria-hidden=\"true\" >\n  <span><\/span>\n<\/span>\n\n  \n<\/a>\n  <\/div>\n<\/div>\n  <\/div>\n    <\/div>\n<\/div>\n  <\/div>\n<\/div>\n  <\/div>\n    <\/div>\n\n<div class=\"e6913-14 x-section  x-hide-lg x-hide-md x-hide-xl\" id=\"mobile-box\" >\n          <\/div>\n\n<div class=\"e6913-15 x-section\" id=\"calculator\" >\n      